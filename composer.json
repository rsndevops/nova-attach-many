{
    "name": "blendbyte/nova-attach-many",
    "description": "Attach Many Nova field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/nova": "^4.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "NovaAttachMany\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaAttachMany\\Providers\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
